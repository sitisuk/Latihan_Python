aldi = int(input("masuk kelereng aldi :"))
budi = aldi -15
anto = 2 * (aldi + budi )
agung = aldi + budi + anto - 5

print("jumlah kelereng adi :",aldi)
print("jumlah kelereng budi :",budi)
print("jumlah kelereng anto :",anto)
print("jumlah kelereng agung :",agung)
